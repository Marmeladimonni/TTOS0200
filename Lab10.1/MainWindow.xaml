<Window x:Class="Lab10._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab10._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="122" Width="249">
    <Grid>
 
        <Button x:Name="btnCars" Content="Cars" HorizontalAlignment="Left" Margin="120,10,0,0" VerticalAlignment="Top" Width="91" Height="23" RenderTransformOrigin="0.013,0.084" Click="btnCars_Click"/>
        <TextBlock x:Name="txtTruckNum" HorizontalAlignment="Left" Margin="10,38,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="90" Height="27" FontWeight="Bold" TextAlignment="Center" FontSize="20"/>
        <TextBlock x:Name="txtCarNum" Margin="120,38,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Height="27" HorizontalAlignment="Left" Width="91" FontSize="20" TextAlignment="Center" FontWeight="Bold"/>
        <Button x:Name="btnTrucks" Content="Trucks" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="90" Height="23" Click="btnTrucks_Click_1"/>

    </Grid>
</Window>
